#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangxinxin/Desktop/next-demos/01_nest-cli/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.12.11_@types+node@22.16.3_typescript@5.8.3/node_modules/ts-node/dist/node_modules:/Users/zhangxinxin/Desktop/next-demos/01_nest-cli/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.12.11_@types+node@22.16.3_typescript@5.8.3/node_modules/ts-node/node_modules:/Users/zhangxinxin/Desktop/next-demos/01_nest-cli/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.12.11_@types+node@22.16.3_typescript@5.8.3/node_modules:/Users/zhangxinxin/Desktop/next-demos/01_nest-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhangxinxin/Desktop/next-demos/01_nest-cli/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.12.11_@types+node@22.16.3_typescript@5.8.3/node_modules/ts-node/dist/node_modules:/Users/zhangxinxin/Desktop/next-demos/01_nest-cli/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.12.11_@types+node@22.16.3_typescript@5.8.3/node_modules/ts-node/node_modules:/Users/zhangxinxin/Desktop/next-demos/01_nest-cli/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.12.11_@types+node@22.16.3_typescript@5.8.3/node_modules:/Users/zhangxinxin/Desktop/next-demos/01_nest-cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-node@10.9.2_@swc+core@1.12.11_@types+node@22.16.3_typescript@5.8.3/node_modules/ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-node@10.9.2_@swc+core@1.12.11_@types+node@22.16.3_typescript@5.8.3/node_modules/ts-node/dist/bin-transpile.js" "$@"
fi
