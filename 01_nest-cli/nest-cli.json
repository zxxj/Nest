{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "assets": [
      "**/*.css",
      { "include": "**/*.html", "exclude": "**/aaa.html", "watchAssets": true }
    ]
  },
  "generateOptions": {
    "flat": false,
    "spec": false
  }
}
